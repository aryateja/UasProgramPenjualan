  this.idtransaksi=idtransaksi;
  this.tgltransaksi=tgltransaksi;
  
  }
  
  public int getIdtransaksi(){
  return idtransaksi=idtransaksi;
  }
  
  public String getTglTr(){
  return tgltransaksi=tgltransaksi;
  }
  }

